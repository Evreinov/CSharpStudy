<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- Какой поставщик? --><!-- 
		 <add key="provider" value="System.Data.OleDb"/>
		 --><!-- Какая строка подключения? --><!-- 
		 <add key="connectionString" value="Provider=SQLNCLI11; Data Source=(localdb)\mssqllocaldb; Initial Catalog=AutoLot; Integrated Security=SSPI"/>-->
		 <!--Какой поставщик?--> 
		<add key="provider" value="System.Data.SqlClient"/>
		 <!--Какая строка подключения?--> 
		<add key="connectionString" value="Data Source=(localdb)\mssqllocaldb; Initial Catalog=AutoLot; Integrated Security=True"/>
	</appSettings>
	<!-- Несколько строк подключения. -->
	<connectionStrings>
		<add name="AutoLotSqlProvider" 
		     connectionString="Data Source=(localdb)\mssqllocaldb; Initial Catalog=AutoLot; Integrated Security=True"/>
		<add name="AutoLotOleDbProvider"
		     connectionString="Provider=SQLNCLI11; Data Source=(localdb)\mssqllocaldb; Initial Catalog=AutoLot; Integrated Security=SSPI"/>
	</connectionStrings>
	<startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
</configuration>